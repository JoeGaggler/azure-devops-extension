{
  "manifestVersion": 1,
  "id": "pingmint-extension",
  "version": "0.1.1",
  "name": "Merge Queue",
  "description": "Pingmint",
  "publisher": "pingmint",
  "categories": [
    "Azure Boards"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/dot-icon.png"
  },
  "contributions": [
    {
      "id": "pingmint-pipeline-mergequeue",
      "type": "ms.vss-web.hub",
      "description": "Merge Queue",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "icon": "images/robotbig.png",
        "name": "Merge Queue",
        "order": 313,
        "uri": "dist/web/mergequeue/index.html"
      }
    },
    {
      "id": "pingmint-pipeline-currentruns",
      "type": "ms.vss-web.hub",
      "description": "Current Runs",
      "targets": [
        "ms.vss-build-web.build-release-hub-group"
      ],
      "properties": {
        "icon": "images/robotbig.png",
        "name": "Current Runs",
        "order": 312,
        "uri": "dist/web/currentruns/index.html"
      }
    }
  ],
  "scopes": [
    "vso.build",
    "vso.build_execute",
    "vso.code",
    "vso.code_status",
    "vso.code_write",
    "vso.graph",
    "vso.identity",
    "vso.pipelineresources_use",
    "vso.profile",
    "vso.project",
    "vso.release",
    "vso.release_manage",
    "vso.work",
    "vso.work_write"
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true,
      "packagePath": "dist"
    },
    {
      "path": "images",
      "addressable": true,
      "packagePath": "images"
    }
  ]
}
